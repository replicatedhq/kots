    "test:pact:api": "cross-env CI=true `npm bin`/jest --runInBand --setupFiles ./pact/setup-api.js --setupFilesAfterEnv ./pact/jest-wrapper.js --testMatch \"**/ship-cloud-api.pact.js\"",
